{
  "version": 6,
  "configurePresets": [
    {
      "name": "default",
      "binaryDir": "build",
      "generator": "Ninja",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_C_COMPILER": "clang",
        "CMAKE_C_FLAGS": "-glldb",
